{
  "LOGGING": {
    "version": 1,
    "disable_existing_loggers": "True",
    "filters": {
      "require_debug_false": {
        "()": "django.utils.log.RequireDebugFalse"
      },
      "require_debug_true": {
        "()": "django.utils.log.RequireDebugTrue"
      }
    },
    "formatters": {
      "simple": {
        "format": "[%(asctime)s] %(levelname)s %(message)s",
        "datefmt": "%Y-%m-%d %H:%M:%S"
      },
      "verbose": {
        "format": "[%(asctime)s] %(levelname)s [%(name)s.%(funcName)s:%(lineno)d] %(message)s",
        "datefmt": "%Y-%m-%d %H:%M:%S"
      }
    },
    "handlers": {
      "console": {
        "level": "DEBUG",
        "filters": [
          "require_debug_true"
        ],
        "class": "logging.StreamHandler",
        "formatter": "simple"
      },
      "main_logfile": {
        "level": "ERROR",
        "filters": [
          "require_debug_false"
        ],
        "class": "logging.handlers.RotatingFileHandler",
        "filename": "/home/setupadmin/adbiz/logs/adbizWebUI_Main.log",

        "backupCount": 5,
        "formatter": "simple"
      },
      "actpbl_logfile": {
        "level": "ERROR",
        "filters": [
          "require_debug_false"
        ],
        "class": "logging.handlers.RotatingFileHandler",
        "filename":  "/home/setupadmin/adbiz/logs/adbizWebUI_Actpbl.log",

        "backupCount": 5,
        "formatter": "simple"
      },
      "actrbl_logfile": {
        "level": "ERROR",
        "filters": [
          "require_debug_false"
        ],
        "class": "logging.handlers.RotatingFileHandler",
        "filename":  "/home/setupadmin/adbiz/logs/adbizWebUI_Actrbl.log",

        "backupCount": 5,
        "formatter": "simple"
      },
      "reco_logfile": {
        "level": "ERROR",
        "filters": [
          "require_debug_false"
        ],
        "class": "logging.handlers.RotatingFileHandler",
        "filename":  "/home/setupadmin/adbiz/logs/adbizWebUI_Reco.log",

        "backupCount": 5,
        "formatter": "simple"
      },
      "dba_logfile": {
        "level": "DEBUG",
        "filters": [
          "require_debug_false",
          "require_debug_true"
        ],
        "class": "logging.handlers.WatchedFileHandler",
        "filename": "/home/setupadmin/adbiz/logs/adbizWebUI_dba.log",
        "formatter": "simple"
      },
      "requests_logfile": {
        "level": "DEBUG",
        "filters": [
          "require_debug_false",
          "require_debug_true"
        ],
        "class": "logging.handlers.WatchedFileHandler",
        "filename": "/home/setupadmin/adbiz/logs/adbizWebUI_Requests.log",
        "formatter": "simple"
      }
    },
    "root": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "loggers": {
      "main": {
        "handlers": [
          "main_logfile"
        ]
      },
       "actpbl": {
        "handlers": [
          "actpbl_logfile"
        ]
      },
       "actrbl": {
        "handlers": [
          "actrbl_logfile"
        ]
      },
       "reco": {
        "handlers": [
          "reco_logfile"
        ]
      },
      "dba": {
        "handlers": [
          "dba_logfile"
        ]
      },
      "django": {
        "handlers": [
          "console"
        ]
      },
      "django.request": {
        "handlers": [
          "requests_logfile"
        ]
      }
    }
  }
}
